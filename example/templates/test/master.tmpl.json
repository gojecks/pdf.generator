{
    "header": "headerHandler",
    "footer": "footerHandler",
    "pageSize": "A4",
    "pageMargins": [20, 100, 20, 100],
    "content": [{
            "text": "{{welcome.messaege}}",
            "processHelpers": ["@underline", "@bold"]
        }, {
            "text": "textContents",
            "style": "header",
            "margin": [0, 3]
        },
        {
            "text": "textContents"
        },
        {
            "table": {
                "body": [
                    ["Testing Auto @underline(layout)"]
                ]
            },
            "layout": "testLayout",
            "processHelpers": ["@underline"]
        },
        "br",
        {
            "text": "textContents",
            "processHelpers": ["@bold"]
        },
        {
            "table": {
                "widths": ["auto", "*"],
                "body": ["tableContent"]
            },
            "processHelpers": ["@underline", "@bold", "@italics", "@boldUnderline", "@boldItalics", "@boldItalicsUnderline", "@underlineItalics"]
        },
        "br",
        {
            "text": "Create and Modify content using handler",
            "fontSize": 21
        },
        {
            "stack": "stackContent",
            "hasLogic": "$stackHandler"
        }
    ],
    "pageBreakBefore": "pageBreakHandler",
    "styles": {
        "subMargin": {
            "margin": [20, 0, 20, 0]
        },
        "alignmentAndBoldHeader": {
            "alignment": "center",
            "bold": true
        },
        "header": {
            "bold": true,
            "color": "blue",
            "fontSize": 11
        },
        "premiumTableClass": {
            "alignment": "center",
            "margin": [0, 5, 0, 0],
            "bold": true
        },
        "singleHeaderDescription": {
            "bold": true,
            "color": "blue",
            "fontSize": 11,
            "margin": [0, 40, 0, 0],
            "alignment": "center"
        },
        "coverageTableClass": {
            "alignment": "center",
            "bold": true
        },
        "mls": {
            "margin": [5, 0]
        }
    },
    "defaultStyle": {
        "fontStyle": 9,
        "alignment": "justify"
    }
}